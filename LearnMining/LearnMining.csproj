<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Authors>Coding Enthusiast</Authors>
    <Company>Autarkysoft</Company>
    <Copyright>Copyright Â© Coding Enthusiast 2019</Copyright>
    <PackageProjectUrl>https://github.com/Autarkysoft/LearnMining</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Autarkysoft/LearnMining/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Autarkysoft/LearnMining</RepositoryUrl>
    <AssemblyVersion>0.9.0.0</AssemblyVersion>
    <FileVersion>0.9.0.0</FileVersion>
    <Version>0.9.0</Version>
    <PackageTags>bitcoin; litecoin; mining; sha256; scrypt</PackageTags>
